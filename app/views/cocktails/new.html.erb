<div class="smth">
  <div class="container">
    <%= simple_form_for (@cocktail) do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
    <%= f.submit "Add a Potion", class: "btn btn-success" %>
    <% end %>
  </div>
</div>
